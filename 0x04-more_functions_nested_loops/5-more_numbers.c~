#include "main.h"

/**
 * more_numbers - print 0-14
 * Return: nothing
 **/

void more_numbers(void)
{
int i;
for (i = 0; i <= 14; i++)
{
_putchar(i);
}
_putchar('\n');
}

/**
 * more_numbers - prints numbers 10 times
 *
 **/
void more_numbers_x10(void)
{
int i;

for (i = '0'; i <= '9'; i++)
{
more_numbers();
}
}
